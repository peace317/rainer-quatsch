{
  "name": "rainer-quatsch",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:test": "cross-env NODE_ENV='test' next dev --port=3010 --turbo",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "docker:db-dev": "docker-compose -f ./docker/docker-compose-db-dev.yaml up -d && db:import",
    "docker:build": "docker-compose -f ./docker/docker-compose.yaml build --progress plain",
    "docker:build:app": "docker-compose -f ./docker/docker-compose.yaml build app --progress plain",
    "docker:build:socket": "docker-compose -f ./docker/docker-compose.yaml build websocket-server --progress plain",
    "docker:deploy": "docker-compose -f ./docker/docker-compose.yaml up -d",
    "docker:deploy:socket": "docker-compose -f ./docker/docker-compose.yaml up -d websocket-server",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"{components,app,service,api-generator}/**/*.{js,ts,tsx,d.ts}\"",
    "lint": "next lint",
    "deploy": "docker build -t rainer-quatsch .",
    "test": "playwright test",
    "test:debug": "playwright test --debug",
    "test:gen": "playwright codegen localhost:3010 --viewport-size=1280,1300",
    "db:import": "node ./prisma/importRuleTables.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "@headlessui/react": "^1.7.17",
    "@livekit/components-react": "^2.0.3",
    "@livekit/components-styles": "^1.0.10",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.13.0",
    "@react-hook/resize-observer": "^1.2.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "chance": "^1.1.11",
    "chart.js": "4.2.1",
    "clsx": "^2.0.0",
    "computed-style": "^0.3.0",
    "date-fns": "^3.5.0",
    "dotenv": "^16.4.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "html-react-parser": "^5.1.7",
    "lodash": "^4.17.21",
    "mongodb": "^6.5.0",
    "next": "^14.2.3",
    "next-auth": "^4.24.7",
    "next-client-cookies": "^1.1.1",
    "next-intl": "^2.22.1",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primereact": "^10.6.3",
    "quill": "^2.0.0-rc.5",
    "react": "^18.2.0",
    "react-big-calendar": "^1.11.6",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-flip-toolkit": "^7.1.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.1",
    "react-timer-hook": "^3.0.7",
    "sharp": "^0.33.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "socket.io-msgpack-parser": "^3.0.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.2",
    "uuid": "^9.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@types/chance": "^1.1.6",
    "@types/lodash": "^4.14.200",
    "@types/react-big-calendar": "^1.8.9",
    "@types/uuid": "^9.0.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-next": "13.5.6",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.71.1"
  }
}
